  pod 'SwiftLint', podspec: 'https://raw.githubusercontent.com/CocoaPods/Specs/master/Specs/4/0/1/SwiftLint/0.27.0/SwiftLint.podspec.json'

  target 'PickleUITests' do
    inherit! :search_paths
  end
end
